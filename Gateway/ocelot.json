{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{url}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/weatherservice/{url}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "ServiceName": "WeatherForecast",
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      },
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "5s",
        "PeriodTimespan": 2,
        "Limit": 3
      },
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Ids4Key",
        "AllowedScopes": []
      }
    }
  ],

  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Scheme": "http",
      "Host": "CONSUL_HOST",
      "Port": "CONSUL_PORT",
      "Type": "Consul",
      "ConfigurationKey": "Oceolot"
    }
  }
}