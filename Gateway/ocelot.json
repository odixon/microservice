{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/weatherforecast",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/{url}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "ServiceName": "WeatherForecast",
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    }
  ],

  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Scheme": "http",
      "Host": "192.168.0.111",
      "Port": 8500,
      "Type": "Consul",
      "ConfigurationKey": "Oceolot"
    }
  }
}