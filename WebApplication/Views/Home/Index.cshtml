@model IEnumerable<WebApplication.Models.Student>

<h1>
    Home / Index
</h1>

<h2>当你看到这行字的时候，说明你已经成功登录了！</h2>

<h3>@Environment.MachineName</h3>

<div id="app">
    <input type="text" placeholder="type keyword" id="keyword"/>
    <button onclick="window.location.href = '/?keyword=' + document.getElementById('keyword').value">Search</button>
</div>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Gender</th>
            <th>Birthday</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Gender</td>
                <td>@item.Birthday</td>
            </tr>
        }
    </tbody>
</table>